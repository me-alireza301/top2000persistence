CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE ARTIST(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,AGE VARCHAR(3),ARTISTSNAME VARCHAR(30),FIRSTNAME VARCHAR(30),LASTNAME VARCHAR(30))
CREATE MEMORY TABLE ALBUM(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,ARTIST_ID INTEGER,CDDBURL VARCHAR(100),PUBLISHDATE TIMESTAMP,TITLE VARCHAR(80))
CREATE MEMORY TABLE SONG(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,ALBUM_ID INTEGER,DURATION INTEGER,LYRICSURL VARCHAR(100),TITLE VARCHAR(100),YOUTUBEURL VARCHAR(100))
ALTER TABLE ARTIST ALTER COLUMN ID RESTART WITH 100
ALTER TABLE ALBUM ALTER COLUMN ID RESTART WITH 100
ALTER TABLE SONG ALTER COLUMN ID RESTART WITH 100
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 60
SET SCHEMA PUBLIC
